        @app.route('/FibNumberReqs')
    def parse_id_request(id):
        if request.method == "PUT":
            id = request.data.get('id','')
            if id:
                request = FibonacciNumbersRequest(id=id)
                request.save()
                response = jsonify({
                   'id' : request.id,
                   'fib_number_input' : request.fib_number_input,
                   'date_requested' : request.date_requested,
                   'date_modified' : request.date_modified,
                   'number_sequence' : request.number_sequence
                })
                response.status_code = 201
                return response
        elif request.method == "DELETE":
            id = request.data.get('id','')
            if id:
                request = FibonacciNumbersRequest(id=id)
                request.delete()
                response = jsonify({
                   'id' : request.id,
                   'fib_number_input' : request.fib_number_input,
                   'date_requested' : request.date_requested,
                   'date_modified' : request.date_modified,
                   'number_sequence' : request.number_sequence
                })
                response.status_code = 201
                return response


                    @app.route('/FibNumberReqs', methods=['POST'])
    def parse_number_request():
        
        if request.method == "POST":
            #return('',200)
            #num_arg = 
            if request.args.get('number') is not None:
                num_arg = request.args.get('number')
                if num_arg < 0 :
                    #error the number can't be less than 0
                    return('', 204)
               
                else:
                    return('', 200)
                    new_req = FibonacciNumbersRequest(fib_number_input = num_arg, number_sequence ='')#  get_fib_sequence(num_arg))
                    db.session.add(new_req)
                    db.session.commit()
                    response = jsonify('ss')#results)
                    response.status_code = 200
                    return response